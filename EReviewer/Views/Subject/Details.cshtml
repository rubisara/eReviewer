@model EReviewer.ViewModels.SubjectViewModels.SubjectViewVM

@{
    ViewData["Title"] = "View Subject";
}

<h3>@ViewData["Title"]</h3>
<hr />
<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label asp-for="Code"></label>
            <label class="form-control">@Model.Code</label>
        </div>
        <div class="form-group">
            <label asp-for="Name"></label>
            <label class="form-control">@Model.Name</label>
        </div>
        <div class="form-group">
            <label asp-for="Description"></label>
            <label class="form-control">@Model.Description</label>
        </div>
        <div class="form-group">
            <a asp-action="Index" class="btn btn-info">Back to List</a> |
            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
        </div>
    </div>
</div>

